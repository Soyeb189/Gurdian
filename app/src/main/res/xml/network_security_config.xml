<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <!--https://stackoverflow.com/questions/57568571/android-studio-javax-net-ssl-sslhandshakeexception-unacceptable-certificate-->
    <base-config>
        <trust-anchors>
            <certificates src="system"/>
            <certificates src="user"/>
        </trust-anchors>
    </base-config>

    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="true">202.40.190.117</domain>
        <domain includeSubdomains="true">10.11.201.180</domain>
    </domain-config>

</network-security-config>